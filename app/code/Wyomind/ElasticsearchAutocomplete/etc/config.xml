<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Core/etc/config.xsd">
    <default>
        <wyomind_elasticsearchautocomplete>
            <license>
                <extension_label>Elasticsearch Autocomplete</extension_label>
                <extension_version>7.3.1</extension_version>
